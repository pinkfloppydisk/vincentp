---
layout: null
---
{% assign publishTime = site.data.published.time %}{
    "published":"{{publishTime}}",
    "webmentions": {% for post in site.posts %}{% assign postTime = post.date | date_to_xmlschema %}{% if postTime >= publishTime %}{% if post.target -%}{% if forloop.first != true %},{% endif %}{
            "source": "{{site.url}}{{ post.url }}",
            "target": "{{post.target}}"
        }{% endif %}{% endif %}{% endfor %}
}
