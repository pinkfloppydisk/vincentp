---
layout: null
---
<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
<channel>
    <title>Vincentp Links</title>
    <description>Syndication feed</description>
    <link>{{ site.url }}</link>
    <atom:link href="{{ site.url }}/feeds/syndication/twitter/links.xml" rel="self" type="application/rss+xml"/>
    {% for post in site.categories.Links limit:20 -%}
        {%- if post.twitter ==  true -%}
      <item>
        <title>{{ post.title | xml_escape }}</title>
        <link>{{ post.url | prepend: site.url }}</link>
        <description>{{ post.summary | xml_escape }}</description>
        <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
        <guid isPermaLink="true">{{ post.url | prepend: site.url }}</guid>
      </item>
        {%- endif -%}
    {%- endfor %}
  </channel>
</rss>

